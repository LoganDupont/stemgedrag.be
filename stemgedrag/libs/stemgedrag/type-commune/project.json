{
  "name": "stemgedrag-type-commune",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/stemgedrag/type-commune/src",
  "prefix": "stemgedrag",
  "tags": ["scope:stemgedrag", "type:type"],
  "projectType": "library",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/stemgedrag/type-commune/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/stemgedrag/type-commune/**/*.ts",
          "libs/stemgedrag/type-commune/**/*.html"
        ]
      }
    }
  }
}
